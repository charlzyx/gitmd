  ],
  "main": "./extension.js",
  "contributes": {
    "commands": []
  },
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/vscode": "^1.79.0",
    "vsce": "^2.15.0"
  },
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "dayjs": "^1.11.8",
    "directory-tree": "^3.5.1",
    "git-rev-sync": "^3.0.2",
    "portfinder": "^1.0.32",
    "rimraf": "^5.0.1",
    "vitepress": "^1.0.0-beta.3"
  }
}
