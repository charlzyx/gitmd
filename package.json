{
  "name": "gitmd",
  "version": "1.2.5",
  "displayName": "gitmd",
  "description": "一个同步 md 文件夹到 git 的小玩意",
  "engines": {
    "vscode": "^1.79.0"
  },
  "publisher": "charlzyx",
  "author": {
    "name": "charlzyx",
    "email": "charlzyx@qq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charlzyx/gitmd.git"
  },
  "bin": {
    "gitmd": "bin.js"
  },
  "scripts": {
